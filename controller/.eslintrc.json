{
  "extends": [
    "standard",
    "react-app",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "indent": [ "error", 2, { "SwitchCase": 1 } ],
    "quotes": [ "error", "double" ],
    "semi": [ "error", "always" ],
    "eol-last": ["error", "never"],
    "react/prop-types": "off"
  }
}